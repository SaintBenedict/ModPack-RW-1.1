<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<ThingDef ParentName="RockBase">
    <defName>Z_MineableAmethyst</defName>
    <label>Amethyst</label>
    <description>Rock containing bits of amethyst.</description>
    <graphicData>
      <texPath>Things/Building/Linked/RockFlecked_Atlas</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
      <color>(78,70,80)</color>
      <colorTwo>(162, 118, 174)</colorTwo>
    </graphicData>
    <statBases>
      <MaxHitPoints>1500</MaxHitPoints>
    </statBases>
    <building>
      <isResourceRock>true</isResourceRock>
      <mineableThing>Z_Amethyst</mineableThing>
      <mineableYield>40</mineableYield>
      <mineableScatterCommonality>0.05</mineableScatterCommonality>
      <mineableScatterLumpSizeRange>
        <min>2</min>
        <max>8</max>
      </mineableScatterLumpSizeRange>
    </building>
  </ThingDef>
  
  
