<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Advanced Biomes</li>
		</mods>

		<match Class="PatchOperationSequence">
			<operations>






			<li Class="PatchOperationAdd"> 
				<xpath>/Defs/ThingDef[defName="AEXP_Alligator"]/race/wildBiomes</xpath>
				<value>
					<PoisonForest>0.01</PoisonForest>
					<Wetland>0.1</Wetland>					
				</value>
			</li>

			<li Class="PatchOperationAdd"> 
				<xpath>/Defs/ThingDef[defName="AEXP_Anaconda"]/race/wildBiomes</xpath>
				<value>					
					<PoisonForest>0.01</PoisonForest>
					<Wetland>0.07</Wetland>	
				</value>
			</li>



			<li Class="PatchOperationAdd"> 
				<xpath>/Defs/ThingDef[defName="AEXP_IndianElephant"]/race/wildBiomes</xpath>
				<value>

					<PoisonForest>0.2</PoisonForest>
					<Wetland>0.1</Wetland>	

				</value>
			</li>	
			


			

		</operations>
		</match>
	</Operation>


</Patch>

