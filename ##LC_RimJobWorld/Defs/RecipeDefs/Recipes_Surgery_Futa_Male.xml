	<RecipeDef ParentName="FutaMakingM">
		<defName>AddVagina</defName>
		<label>add average vagina</label>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>Medicine</li>
					</categories>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Vagina</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
