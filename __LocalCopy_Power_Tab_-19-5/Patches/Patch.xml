<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/ThingDef[comps/li[@Class = "CompProperties_Power"]]/comps</xpath>
		<value>
			<li Class="Compilatron.CompProperties_PowerTracker" />
		</value>
	</Operation>
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/ThingDef[comps/li[@Class = "CompProperties_Battery"]]/comps</xpath>
		<value>
			<li Class="Compilatron.CompProperties_PowerTracker" />
		</value>
	</Operation>
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/ThingDef[comps/li[@Class = "Compilatron.CompProperties_PowerTracker"] and not(inspectorTabs)]</xpath>
		<value>
			<inspectorTabs/>
		</value>
	</Operation>
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/ThingDef[comps/li[@Class = "Compilatron.CompProperties_PowerTracker"]]/inspectorTabs</xpath>
		<value>
			<li>Compilatron.ITab_Power</li>
		</value>
	</Operation>
</Patch>