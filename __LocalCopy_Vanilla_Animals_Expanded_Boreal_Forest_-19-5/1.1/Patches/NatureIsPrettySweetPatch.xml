<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Nature's Pretty Sweet</li>
		</mods>

		<match Class="PatchOperationSequence">
			<operations>
	




			

			<li Class="PatchOperationAdd"> 
				<xpath>/Defs/ThingDef[defName="AEXP_Otter"]/race/wildBiomes</xpath>
				<value>
					<TKKN_RedwoodForest>1</TKKN_RedwoodForest>
					<TKKN_Grasslands>0.03</TKKN_Grasslands>
					
				</value>
			</li>
			
			<li Class="PatchOperationAdd"> 
				<xpath>/Defs/ThingDef[defName="AEXP_ArcticCoyote"]/race/wildBiomes</xpath>
				<value>					
					<TKKN_RedwoodForest>1</TKKN_RedwoodForest>
					<TKKN_Grasslands>0.03</TKKN_Grasslands>
				</value>
			</li>
			
			<li Class="PatchOperationAdd"> 
				<xpath>/Defs/ThingDef[defName="AEXP_BlackBear"]/race/wildBiomes</xpath>
				<value>	
					<TKKN_RedwoodForest>0.1</TKKN_RedwoodForest>
					<TKKN_Grasslands>0.07</TKKN_Grasslands>
										
				</value>
			</li>	

		
			
		</operations>
		</match>
	</Operation>


</Patch>

