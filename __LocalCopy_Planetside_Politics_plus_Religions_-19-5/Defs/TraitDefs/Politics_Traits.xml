<?xml version="1.0" encoding="utf-8" ?>
<Defs>

<!-- ========================== Individual Liberty ============================ -->

  <PoliticsandReligions.TraitDef_PoliticsTrait>
    	<defName>PoliticsRepublican</defName>
		<commonality>20.0</commonality>
    	<degreeDatas>
      	<li>
        	<label>Republican</label>
        	<description>{PAWN_nameDef} believes a republic to be the ideal form of governance.</description>
      	</li>
    	</degreeDatas>
		<conflictingTraits>
			<li>PoliticsDemocrat</li>
			<li>PoliticsSocialist</li>
			<li>PoliticsCommunist</li>
			<li>PoliticsFascist</li>
			<li>PoliticsAnarchist</li>
    		<li>PoliticsMonarchist</li>
			<li>PoliticsTheocrat</li>
			<li>PoliticsDespot</li>
		</conflictingTraits>
		<PoliticalAdjacent>
			<li>PoliticsDemocrat</li>
		</PoliticalAdjacent>
		<PoliticalMisguided>
		    <li>PoliticsAnarchist</li>
		</PoliticalMisguided>
		<PoliticalRivalry>
    		<li>PoliticsSocialist</li>
		</PoliticalRivalry>
		<PoliticalEnemy>
			<li>PoliticsFascist</li>
    		<li>PoliticsCommunist</li>
    		<li>PoliticsMonarchist</li>			    		
			<li>PoliticsTheocrat</li>
    		<li>PoliticsDespot</li>
		</PoliticalEnemy>
		<isTribalIdeology>false</isTribalIdeology>
		<isPirateIdeology>false</isPirateIdeology>
    </PoliticsandReligions.TraitDef_PoliticsTrait>

