<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Advanced Biomes</li>
		</mods>

		<match Class="PatchOperationSequence">
			<operations>






			<li Class="PatchOperationAdd"> 
				<xpath>/Defs/ThingDef[defName="AEXP_Gorilla"]/race/wildBiomes</xpath>
				<value>
					<PoisonForest>0.01</PoisonForest>
					<Wetland>0.1</Wetland>					
				</value>
			</li>

			<li Class="PatchOperationAdd"> 
				<xpath>/Defs/ThingDef[defName="AEXP_Jaguar"]/race/wildBiomes</xpath>
				<value>					
					<PoisonForest>0.05</PoisonForest>
					<Wetland>0.1</Wetland>	
				</value>
			</li>



			<li Class="PatchOperationAdd"> 
				<xpath>/Defs/ThingDef[defName="AEXP_Lemur"]/race/wildBiomes</xpath>
				<value>

					<PoisonForest>0.2</PoisonForest>
					<Wetland>0.1</Wetland>	

				</value>
			</li>	
			<li Class="PatchOperationAdd"> 
				<xpath>/Defs/ThingDef[defName="AEXP_Mandrill"]/race/wildBiomes</xpath>
				<value>					
					<PoisonForest>0.07</PoisonForest>
					<Wetland>0.1</Wetland>	
				</value>
			</li>		
			<li Class="PatchOperationAdd"> 
				<xpath>/Defs/ThingDef[defName="AEXP_Tapir"]/race/wildBiomes</xpath>
				<value>
					<PoisonForest>0.2</PoisonForest>
					<Wetland>0.15</Wetland>	
				</value>
			</li>			
			<li Class="PatchOperationAdd"> 
				<xpath>/Defs/ThingDef[defName="AEXP_Tiger"]/race/wildBiomes</xpath>
				<value>
					<PoisonForest>0.07</PoisonForest>
					<Wetland>0.07</Wetland>	
				</value>
			</li>	


			

		</operations>
		</match>
	</Operation>


</Patch>

