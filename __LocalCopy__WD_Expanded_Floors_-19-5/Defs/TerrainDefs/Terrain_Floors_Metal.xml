<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!-- ================= Metal Different ================= --> 
  
	<TerrainDef ParentName="FloorBase">
		<defName>Concrete</defName>
		<label>concrete</label>
		<description>Rough concrete. It is fast to walk on, cheap, and quick to build, but quite ugly.</description>
		<texturePath>Terrain/Surfaces/Concrete</texturePath>
		<edgeType>Hard</edgeType>
		<renderPrecedence>70</renderPrecedence>
		<statBases>
			<WorkToBuild>180</WorkToBuild>
		</statBases>
		<costList>	
			<Steel>1</Steel>
		</costList>
		<designationHotKey>Misc6</designationHotKey>
    <researchPrerequisites>
      <li>Stonecutting</li>
    </researchPrerequisites>
	</TerrainDef>

	<TerrainDef ParentName="FloorBase">
		<defName>PavedTile</defName>
		<label>paved tile</label>
		<renderPrecedence>230</renderPrecedence>
		<description>Concrete tiles. Cheap and quick to move around on, but neutral in terms of looks and slow to build.</description>
		<texturePath>Terrain/Surfaces/PavedTile</texturePath>
		<statBases>
			<WorkToBuild>180</WorkToBuild>
		</statBases>
		<costList>
			<Steel>1</Steel>
		</costList>
		<designationHotKey>Misc5</designationHotKey>
    <researchPrerequisites>
      <li>Stonecutting</li>
    </researchPrerequisites>
	</TerrainDef> 

	<TerrainDef ParentName="FloorBase">
		<defName>WDMetalSheets</defName>
		<label>metal sheets</label>
		<renderPrecedence>243</renderPrecedence> 
		<description>Floor made of simple metal sheets. Don't look great, but cheap and fast to build. Good to cover big industrial areas.</description>
		<texturePath>Terrain/Surfaces/Metal/MetalSheets</texturePath>
	    <color>(0.369, 0.369, 0.369)</color>		
		<statBases>
			<WorkToBuild>360</WorkToBuild>	
		</statBases>
		<costList>
		    <Steel>2</Steel>
		</costList>
    <constructEffect>ConstructMetal</constructEffect>			
    <researchPrerequisites>
      <li>Smithing</li>
    </researchPrerequisites>
	</TerrainDef>  	
	
